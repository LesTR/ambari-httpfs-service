{
  "HTTPFS": {
    "service": [],
    "HTTPFS_GATEWAY": [
      {
        "name": "httpfs_server_process",
        "label": "HTTPFS server Process",
        "description": "This host-level alert is triggered if the HTTPFS server cannot be determined to be up.",
        "interval": 1,
        "scope": "HOST",
        "source": {
          "type": "PORT",
          "uri": "{{httpfs/httpfs.port}}",
          "default_port": 14000,
          "reporting": {
            "ok": {
              "text": "TCP OK - {0:.3f}s response on port {1}"
            },
            "warning": {
              "text": "TCP OK - {0:.3f}s response on port {1}",
              "value": 1.5
            },
            "critical": {
              "text": "Connection failed: {0} to {1}:{2}",
              "value": 5.0
            }
          }
        }
      }
    ]
  }
}
